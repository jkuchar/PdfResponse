{
	"name": "jkuchar/pdfresponse",
	"description": "PdfResponse is wrapper of mPDF for Nette.",
	"license": "LGPL-2.0-or-later",
	"scripts": {
		"verify": [
			"@phpstan",
			"@tester"
		],
		"tester": "vendor/bin/tester test",
		"phpstan": "vendor/bin/phpstan analyze src -c phpstan.neon --memory-limit 1G -l 1 --error-format compact"
	},
	"authors": [
		{
			"name": "Jan Kucha≈ô",
			"homepage": "http://www.jankuchar.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"PdfResponse\\": "src"
		}
	},
	"require": {
		"php": "~7.1.0|~7.2.0|~7.3.0|~7.4.0|~8.0.0|~8.1.0|~8.2.0",
		"nette/utils": "~2.4.0|~2.5.0|~3.0.0|~3.1.0|~3.2.0",
		"nette/http": "~2.4.0|~3.0.0|~3.1.0|~3.2.0",
		"nette/application": "~2.4.0|~3.0.0|~3.1.0",
		"mpdf/mpdf": "^8.0",
		"paquettg/php-html-parser": "^2.1.0"
	},
	"require-dev": {
		"nette/tester": "^2.3.1",
		"phpstan/phpstan": "^0.12.7",
		"grifart/phpstan-oneline": "^0.3.0"
	}
}
